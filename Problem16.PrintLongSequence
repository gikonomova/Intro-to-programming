// Intro-to-programming
// Programming Basics September

using System;

class PrintLongSequence
{
    static void Main()
    {
        for (int i = 0; i <= 1000; i++)
        {
            if (i % 2 == 0)
            {
                Console.WriteLine(i);
            }
            else
            {
                Console.WriteLine(-i);
            }
        }
        Console.ReadLine();
    }
}
